<div class="jumbotron">
    <h1>Register</h1>
</div>
<div class="row">
    <div class="col-md-2">
    </div>
    <div class="col-md-8">
        <div class="row">
            <div class="thumbnail">
                <div class="caption">
                    <form class="form-horizontal">
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Name</label>
                            <div class="col-sm-10">
                                <input class="form-control" id="name" type="text" placeholder="Enter your name...">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Email ID</label>
                            <div class="col-sm-10">
                                <input class="form-control" id="email" type="text" placeholder="Enter your Email ID...">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">User Name</label>
                            <div class="col-sm-10">
                                <input class="form-control" id="username" type="text" placeholder="Enter your user name...">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Password</label>
                            <div class="col-sm-10">
                                <input class="form-control" id="password" type="password" placeholder="Enter your password...">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="disabledInput" class="col-sm-2 control-label"></label>
                            <div class="col-sm-10">
                                <button type="button" id="btnRegister" class="btn btn-default">Register</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-2">
    </div>
</div>
